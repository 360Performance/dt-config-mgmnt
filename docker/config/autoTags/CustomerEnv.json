{
    "name": "CustomerEnv",
    "rules": [
        {
            "type": "HOST",
            "enabled": true,
            "valueFormat": "{HostGroup:Name}",
            "propagationTypes": [
                "HOST_TO_PROCESS_GROUP_INSTANCE"
            ],
            "conditions": [
                {
                    "key": {
                        "attribute": "HOST_TAGS"
                    },
                    "comparisonInfo": {
                        "type": "TAG",
                        "operator": "TAG_KEY_EQUALS",
                        "value": {
                            "context": "ENVIRONMENT",
                            "key": "client"
                        },
                        "negate": false
                    }
                }
            ]
        },
        {
            "type": "SERVICE",
            "enabled": true,
            "valueFormat": "{HostGroup:Name}",
            "propagationTypes": [
                "SERVICE_TO_PROCESS_GROUP_LIKE",
                "SERVICE_TO_HOST_LIKE"
            ],
            "conditions": [
                {
                    "key": {
                        "attribute": "HOST_TAGS"
                    },
                    "comparisonInfo": {
                        "type": "TAG",
                        "operator": "TAG_KEY_EQUALS",
                        "value": {
                            "context": "ENVIRONMENT",
                            "key": "client"
                        },
                        "negate": false
                    }
                }
            ]
        }
    ]
}